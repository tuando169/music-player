{
    "version": 1,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}